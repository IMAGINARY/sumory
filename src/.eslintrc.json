{
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline", { "functions": "never" }],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": true, "peerDependencies": false, "bundledDependencies": true}]
  }
}
